{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug AutoDrive",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/AutoDrive",
			"args": ["2"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/build",
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/bin/gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "CMake Build"
		}
	]
}